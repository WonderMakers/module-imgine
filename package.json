{
  "name": "imgine",
  "version": "1.2.0",
  "description": "Generate & convert images",
  "repository": {
    "type": "git",
    "url": "https://git.hzberg.com/modules/imgine"
  },
  "type": "module",
  "main": "dist/nuxt.imagine.module.js",
  "scripts": {
    "test": "mocha --require esm --timeout 60000",
    "lib-node-converter": "webpack --config ./src/node-converter/webpack.config.cjs",
    "lib-nuxt-module": "webpack --config ./src/nuxt-module/webpack.config.cjs",
    "lib-nuxt-plugin": "webpack --config ./src/nuxt-plugin/webpack.config.cjs",
    "lib-loader": "webpack --config ./src/webpack-loader/webpack.config.cjs",
    "lib-nuxt-components": "webpack --config ./src/nuxt-components/webpack.config.cjs",
    "libs": "npm run lib-node-converter && npm run lib-nuxt-module && npm run lib-nuxt-plugin && npm run lib-nuxt-components && npm run lib-loader",
    "build": "npm run test && npm run libs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "object-hash": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.3.1",
    "core-js": "3.9.1",
    "css-loader": "^6.8.1",
    "esm": "3.2.25",
    "memfs": "3.3.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "10.2.0",
    "node-fetch": "3.3.1",
    "node-loader": "1.0.2",
    "sharp": "0.32.1",
    "vue-loader": "15.9.8",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.6.12",
    "webpack": "5.88.0",
    "webpack-cli": "5.1.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
